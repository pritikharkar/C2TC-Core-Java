import java.util.*;  

public class ScannerExample {  

public static void main(String args[]){  

          Scanner in = new Scanner(System.in);  

          System.out.print("Priti arun kharkar ");  

          String name = in.nextLine();  

          System.out.println("Priti " + name);             

          in.close();             

          }  

}  


